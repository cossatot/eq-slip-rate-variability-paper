	file = {Clark et al. - 2012 - Long-term behaviour of Australian stable continent.pdf:/Users/itchy/Zotero/storage/CPHCKTZ2/Clark et al. - 2012 - Long-term behaviour of Australian stable continent.pdf:application/pdf}
}

@article{dolan_long-range_2007,
