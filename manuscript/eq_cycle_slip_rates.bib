	author = {Rizza, M. and Ritz, J.-F. and Prentice, C. and Vassallo, R. and Braucher, R. and Larroque, C. and Arzhannikova, A. and Arzhannikov, S. and Mahan, S. and Massault, M. and Michelot, J.-L. and Todbileg, M. and Team, Aster},
	month = feb,
	year = {2015},
	pages = {72--93},
	file = {Rizza_Ritz_Prentice_Vassallo_Braucher_Larroque_Arzhannikova_Arzhannikov_Mahan_Massault_Michelot_Todbileg_Team_et_al_2015_Earthquake_Geology_of_the_Bulnay_Fault_(Mongolia).pdf:/Users/itchy/Zotero/storage/L9W2SW52/Rizza_Ritz_Prentice_Vassallo_Braucher_Larroque_Arzhannikova_Arzhannikov_Mahan_Massault_Michelot_Todbileg_Team_et_al_2015_Earthquake_Geology_of_the_Bulnay_Fault_(Mongolia).pdf:application/pdf;Snapshot:/Users/itchy/Zotero/storage/TS6IKV5T/72.html:text/html}
