	pages = {449--463},
	file = {Lifton et al. - 2015 - Latest Pleistocene and Holocene slip rates on the .pdf:/Users/itchy/Zotero/storage/5DGFAZAG/Lifton et al. - 2015 - Latest Pleistocene and Holocene slip rates on the .pdf:application/pdf}
}

@article{ogata_estimating_1999,
