@article{leonard_earthquake_2010,
	title = {Earthquake {Fault} {Scaling}: {Self}-{Consistent} {Relating} of {Rupture} {Length}, {Width}, {Average} {Displacement}, and {Moment} {Release}},
	volume = {100},
	issn = {0037-1106, 1943-3573},
	shorttitle = {Earthquake {Fault} {Scaling}},
	url = {http://bssa.geoscienceworld.org.www2.lib.ku.edu/content/100/5A/1971},
	doi = {10.1785/0120090189},
	abstract = {In this paper, I propose the scaling relation W=C1Lβ (where β≈2/3) to describe the scaling of rupture width with rupture length. I also propose a new displacement relation {\textless}img class="highwire-embed" alt="Embedded Image" src="http://bssa.geoscienceworld.org.www2.lib.ku.edu/sites/default/files/highwire/ssabull/100/5A/1971/embed/inline-graphic-1.gif"/{\textgreater}, where A is the area (LW). By substituting these equations into the definition of seismic moment ({\textless}img class="highwire-embed" alt="Embedded Image" src="http://bssa.geoscienceworld.org.www2.lib.ku.edu/sites/default/files/highwire/ssabull/100/5A/1971/embed/inline-graphic-2.gif"/{\textgreater}), I have developed a series of self-consistent equations that describe the scaling between seismic moment, rupture area, length, width, and average displacement. In addition to β, the equations have only two variables, C1 and C2, which have been estimated empirically for different tectonic settings. The relations predict linear log–log relationships, the slope of which depends only on β.
These new scaling relations, unlike previous relations, are self-consistent, in that they enable moment, rupture length, width, area, and displacement to be estimated from each other and with these estimates all being consistent with the definition of seismic moment. I interpret C1 as depending on the size at which a rupture transitions from having a constant aspect ratio to following a power law and C2 as depending on the displacement per unit area of fault rupture and so static stress drop. It is likely that these variables differ between tectonic environments; this might explain much of the scatter in the empirical data.
I suggest that these relations apply to all faults. For small earthquakes (M{\textless}∼5) β=1, in which case L3 fault scaling applies. For larger (M{\textgreater}∼5) earthquakes β=2/3, so L2.5 applies. For dip-slip earthquakes this scaling applies up to the largest events. For very large (M{\textgreater}∼7.2) strike-slip earthquakes, which are fault width-limited, β=0 and assuming {\textless}img class="highwire-embed" alt="Embedded Image" src="http://bssa.geoscienceworld.org.www2.lib.ku.edu/sites/default/files/highwire/ssabull/100/5A/1971/embed/inline-graphic-3.gif"/{\textgreater}, then L1.5 scaling applies. In all cases, M0∝A1.5 fault scaling applies.},
	language = {en},
	number = {5A},
	urldate = {2017-10-18},
	journal = {Bulletin of the Seismological Society of America},
	author = {Leonard, Mark},
	month = oct,
	year = {2010},
	pages = {1971--1988},
	file = {Full Text PDF:/Users/itchy/Zotero/storage/3KKC4TEG/Leonard - 2010 - Earthquake Fault Scaling Self-Consistent Relating.pdf:application/pdf;Snapshot:/Users/itchy/Zotero/storage/95KQ99CD/1971.html:text/html}
