@article{zinke_highly_2018,
	title = {Highly {Variable} {Latest} {Pleistocene}-{Holocene} {Incremental} {Slip} {Rates} on the {Awatere} {Fault} at {Saxton} {River}, {South} {Island}, {New} {Zealand}, {Revealed} by {Lidar} {Mapping} and {Luminescence} {Dating}},
